@model List<PoliziaApp.Models.PuntiPerPersona>

@{
}

<table class="table">
    <thead>
        <tr>
            <th scope="col">Nominativo</th>
            <th scope="col">Punti Decurtati</th>


        </tr>
    </thead>
    <tbody>
        @foreach (PuntiPerPersona verbale in Model)
        {
            <tr>
                <td>@verbale.Nome @verbale.Cognome</td>
                <td>@verbale.PuntiDecurtati</td>

            </tr>

        }
    </tbody>
</table>